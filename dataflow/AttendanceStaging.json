				"Attendance2024 select(mapColumn(",
				"          SchoolYear,",
				"          OrganizationLevel,",
				"          County,",
				"          DistrictCode,",
				"          DistrictName,",
				"          DistrictOrganizationId,",
				"          SchoolCode,",
				"          SchoolName,",
				"          SchoolOrganizationId,",
				"          CurrentSchoolType,",
				"          StudentGroupType,",
				"          StudentGroup,",
				"          GradeLevel,",
				"          Measure,",
				"          DATReason = DAT_Reason,",
				"          Numerator,",
				"          Denominator,",
				"          DataAsOf",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"union1 sink(allowSchemaDrift: true,",
