				"          DistrictName,",
				"          DistrictOrganizationId,",
				"          SchoolCode,",
				"          SchoolName,",
				"          SchoolOrganizationId,",
				"          CurrentSchoolType,",
				"          StudentGroupType,",
				"          StudentGroup,",
				"          GradeLevel,",
				"          Measure,",
